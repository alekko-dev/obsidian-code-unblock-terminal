{
  "name": "obsidian-code-unblock-terminal",
  "version": "0.1.0",
  "description": "Obsidian plugin that integrates terminals with seamless code block execution",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "terminal",
    "shell",
    "code-execution"
  ],
  "author": "Aleksandr Kokin",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "builtin-modules": "^3.3.0",
    "electron": "^37.0.0",
    "esbuild": "^0.19.10",
    "obsidian": "^1.4.11",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "node-pty": "^1.1.0-beta35",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0"
  }
}
